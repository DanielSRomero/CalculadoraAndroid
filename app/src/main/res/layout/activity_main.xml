<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="10"
                android:text="0"
                android:textSize="50sp"
                android:background="@drawable/pantalla"
                />

            <Button
                android:id="@+id/button"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:backgroundTint="@null"
                android:background="@drawable/selector_del"
                android:textSize="30sp"
                android:text="del" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="10"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button5"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_secundario"
                    android:textSize="30sp"
                    android:text="AC"
                    app:cornerRadius="20dp" />

                <Button
                    android:id="@+id/button4"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_secundario"
                    android:textSize="30sp"
                    android:text="+/-"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button3"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_secundario"
                    android:textSize="30sp"
                    android:text="%"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button2"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_secundario"
                    android:textSize="30sp"
                    android:text="/"
                    app:cornerRadius="20dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button9"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text="7"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button8"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text="8"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button6"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text="9"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button7"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_secundario"
                    android:textSize="30sp"
                    android:text="X"
                    app:cornerRadius="20dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button13"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text="4"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button12"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text="5"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button11"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text="6"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button10"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_secundario"
                    android:textSize="30sp"
                    android:text="-"
                    app:cornerRadius="20dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button14"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text="1"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button15"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text="2"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button16"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text="3"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button17"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_secundario"
                    android:textSize="30sp"
                    android:text="+"
                    app:cornerRadius="20dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button18"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text="0"
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button19"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_principal"
                    android:textSize="30sp"
                    android:text=","
                    app:cornerRadius="20dp"/>

                <Button
                    android:id="@+id/button20"
                    style="?attr/materialButtonOutlinedStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="@null"
                    android:background="@drawable/selector_terciario"
                    android:textSize="30sp"
                    android:text="="
                    app:cornerRadius="20dp"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>